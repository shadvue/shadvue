      "content": "<script setup lang=\"ts\">\r\nimport {\r\n  Frame,\r\n  LifeBuoy,\r\n  Map,\r\n  PanelLeftClose,\r\n  PanelLeftOpen,\r\n  PieChart,\r\n  Send,\r\n} from \"lucide-vue-next\"\r\n\r\nimport { ref } from \"vue\"\r\nimport { Button } from \"@/registry/default/ui/button\"\r\nimport {\r\n  Sidebar,\r\n  SidebarContent,\r\n  SidebarGroup,\r\n  SidebarGroupContent,\r\n  SidebarGroupLabel,\r\n  SidebarInset,\r\n  SidebarMenu,\r\n  SidebarMenuButton,\r\n  SidebarMenuItem,\r\n  SidebarProvider,\r\n} from \"@/registry/default/ui/sidebar\"\r\n\r\nconst projects = [\r\n  {\r\n    name: \"Design Engineering\",\r\n    url: \"#\",\r\n    icon: Frame,\r\n  },\r\n  {\r\n    name: \"Sales & Marketing\",\r\n    url: \"#\",\r\n    icon: PieChart,\r\n  },\r\n  {\r\n    name: \"Travel\",\r\n    url: \"#\",\r\n    icon: Map,\r\n  },\r\n  {\r\n    name: \"Support\",\r\n    url: \"#\",\r\n    icon: LifeBuoy,\r\n  },\r\n  {\r\n    name: \"Feedback\",\r\n    url: \"#\",\r\n    icon: Send,\r\n  },\r\n]\r\n\r\nconst open = ref(true)\r\n</script>\r\n\r\n<template>\r\n  <SidebarProvider v-model:open=\"open\">\r\n    <Sidebar>\r\n      <SidebarContent>\r\n        <SidebarGroup>\r\n          <SidebarGroupLabel>Projects</SidebarGroupLabel>\r\n          <SidebarGroupContent>\r\n            <SidebarMenu>\r\n              <SidebarMenuItem v-for=\"project in projects\" :key=\"project.name\">\r\n                <SidebarMenuButton as-child>\r\n                  <a :href=\"project.url\">\r\n                    <component :is=\"project.icon\" />\r\n                    <span>{{ project.name }}</span>\r\n                  </a>\r\n                </SidebarMenuButton>\r\n              </SidebarMenuItem>\r\n            </SidebarMenu>\r\n          </SidebarGroupContent>\r\n        </SidebarGroup>\r\n      </SidebarContent>\r\n    </Sidebar>\r\n    <SidebarInset>\r\n      <header class=\"flex items-center h-12 px-4 justify-between\">\r\n        <Button\r\n          size=\"sm\"\r\n          variant=\"ghost\"\r\n          @click=\"open = !open\"\r\n        >\r\n          <PanelLeftClose v-if=\"open\" />\r\n          <PanelLeftOpen v-else />\r\n          <span>{{ open ? 'Close' : 'Open' }} Sidebar</span>\r\n        </Button>\r\n      </header>\r\n    </SidebarInset>\r\n  </SidebarProvider>\r\n</template>\r\n",
