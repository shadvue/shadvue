  "files": [
    {
      "path": "examples/ComboboxDemo.vue",
      "content": "<script setup lang=\"ts\">\r\nimport { Check, Search } from \"lucide-vue-next\"\r\nimport { cn } from \"@/lib/utils\"\r\nimport { Combobox, ComboboxAnchor, ComboboxEmpty, ComboboxGroup, ComboboxInput, ComboboxItem, ComboboxItemIndicator, ComboboxList } from \"@/registry/new-york/ui/combobox\"\r\n\r\nconst frameworks = [\r\n  { value: \"next.js\", label: \"Next.js\" },\r\n  { value: \"sveltekit\", label: \"SvelteKit\" },\r\n  { value: \"nuxt\", label: \"Nuxt\" },\r\n  { value: \"remix\", label: \"Remix\" },\r\n  { value: \"astro\", label: \"Astro\" },\r\n]\r\n</script>\r\n\r\n<template>\r\n  <Combobox by=\"label\">\r\n    <ComboboxAnchor>\r\n      <div class=\"relative w-full max-w-sm items-center\">\r\n        <ComboboxInput class=\"pl-9\" :display-value=\"(val) => val?.label ?? ''\" placeholder=\"Select framework...\" />\r\n        <span class=\"absolute start-0 inset-y-0 flex items-center justify-center px-3\">\r\n          <Search class=\"size-4 text-muted-foreground\" />\r\n        </span>\r\n      </div>\r\n    </ComboboxAnchor>\r\n\r\n    <ComboboxList>\r\n      <ComboboxEmpty>\r\n        No framework found.\r\n      </ComboboxEmpty>\r\n\r\n      <ComboboxGroup>\r\n        <ComboboxItem\r\n          v-for=\"framework in frameworks\"\r\n          :key=\"framework.value\"\r\n          :value=\"framework\"\r\n        >\r\n          {{ framework.label }}\r\n\r\n          <ComboboxItemIndicator>\r\n            <Check :class=\"cn('ml-auto h-4 w-4')\" />\r\n          </ComboboxItemIndicator>\r\n        </ComboboxItem>\r\n      </ComboboxGroup>\r\n    </ComboboxList>\r\n  </Combobox>\r\n</template>\r\n",
      "type": "registry:example",
      "target": ""
    }
  ]
}
