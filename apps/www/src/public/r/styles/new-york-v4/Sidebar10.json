\r\n                      <span>{{ page.name }}</span>\r\n                    </a>\r\n                  </SidebarMenuSubButton>\r\n                </SidebarMenuSubItem>\r\n              </SidebarMenuSub>\r\n            </CollapsibleContent>\r\n          </SidebarMenuItem>\r\n        </Collapsible>\r\n\r\n        <SidebarMenuItem>\r\n          <SidebarMenuButton class=\"text-sidebar-foreground/70\">\r\n            <MoreHorizontal />\r\n            <span>More</span>\r\n          </SidebarMenuButton>\r\n        </SidebarMenuItem>\r\n      </SidebarMenu>\r\n    </SidebarGroupContent>\r\n  </SidebarGroup>\r\n</template>\r\n",
