    {
      "path": "blocks/Sidebar10/components/NavFavorites.vue",
      "content": "<script setup lang=\"ts\">\r\nimport {\r\n  ArrowUpRight,\r\n  Link,\r\n  MoreHorizontal,\r\n  StarOff,\r\n  Trash2,\r\n} from \"lucide-vue-next\"\r\n\r\nimport {\r\n  DropdownMenu,\r\n  DropdownMenuContent,\r\n  DropdownMenuItem,\r\n  DropdownMenuSeparator,\r\n  DropdownMenuTrigger,\r\n} from \"@/registry/new-york/ui/dropdown-menu\"\r\nimport {\r\n  SidebarGroup,\r\n  SidebarGroupLabel,\r\n  SidebarMenu,\r\n  SidebarMenuAction,\r\n  SidebarMenuButton,\r\n  SidebarMenuItem,\r\n  useSidebar,\r\n} from \"@/registry/new-york/ui/sidebar\"\r\n\r\ndefineProps<{\r\n  favorites: {\r\n    name: string\r\n    url: string\r\n    emoji: string\r\n  }[]\r\n}>()\r\n\r\nconst { isMobile } = useSidebar()\r\n</script>\r\n\r\n<template>\r\n  <SidebarGroup class=\"group-data-[collapsible=icon]:hidden\">\r\n    <SidebarGroupLabel>Favorites</SidebarGroupLabel>\r\n    <SidebarMenu>\r\n      <SidebarMenuItem v-for=\"item in favorites\" :key=\"item.name\">\r\n        <SidebarMenuButton as-child>\r\n          <a :href=\"item.url\" :title=\"item.name\">\r\n            <span>{{ item.emoji }}</span>\r\n            <span>{{ item.name }}</span>\r\n          </a>\r\n        </SidebarMenuButton>\r\n        <DropdownMenu>\r\n          <DropdownMenuTrigger as-child>\r\n            <SidebarMenuAction show-on-hover>\r\n              <MoreHorizontal />\r\n              <span class=\"sr-only\">More</span>\r\n            </SidebarMenuAction>\r\n          </DropdownMenuTrigger>\r\n          <DropdownMenuContent\r\n            class=\"w-56 rounded-lg\"\r\n            :side=\"isMobile ? 'bottom' : 'right'\"\r\n            :align=\"isMobile ? 'end' : 'start'\"\r\n          >\r\n            <DropdownMenuItem>\r\n              <StarOff class=\"text-muted-foreground\" />\r\n              <span>Remove from Favorites</span>\r\n            </DropdownMenuItem>\r\n            <DropdownMenuSeparator />\r\n            <DropdownMenuItem>\r\n              <Link class=\"text-muted-foreground\" />\r\n              <span>Copy Link</span>\r\n            </DropdownMenuItem>\r\n            <DropdownMenuItem>\r\n              <ArrowUpRight class=\"text-muted-foreground\" />\r\n              <span>Open in New Tab</span>\r\n            </DropdownMenuItem>\r\n            <DropdownMenuSeparator />\r\n            <DropdownMenuItem>\r\n              <Trash2 class=\"text-muted-foreground\" />\r\n              <span>Delete</span>\r\n            </DropdownMenuItem>\r\n          </DropdownMenuContent>\r\n        </DropdownMenu>\r\n      </SidebarMenuItem>\r\n\r\n      <SidebarMenuItem>\r\n        <SidebarMenuButton class=\"text-sidebar-foreground/70\">\r\n          <MoreHorizontal />\r\n          <span>More</span>\r\n        </SidebarMenuButton>\r\n      </SidebarMenuItem>\r\n    </SidebarMenu>\r\n  </SidebarGroup>\r\n</template>\r\n",
      "type": "registry:component",
      "target": ""
    },
