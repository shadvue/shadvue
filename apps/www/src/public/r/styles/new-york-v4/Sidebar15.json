    {
      "path": "blocks/Sidebar15/components/NavFavorites.vue",
      "content": "<script setup lang=\"ts\">\r\nimport {\r\n  ArrowUpRight,\r\n  Link,\r\n  MoreHorizontal,\r\n  StarOff,\r\n  Trash2,\r\n} from \"lucide-vue-next\"\r\n\r\nimport {\r\n  DropdownMenu,\r\n  DropdownMenuContent,\r\n  DropdownMenuItem,\r\n  DropdownMenuSeparator,\r\n  DropdownMenuTrigger,\r\n} from \"@/registry/new-york/ui/dropdown-menu\"\r\nimport {\r\n  SidebarGroup,\r\n  SidebarGroupLabel,\r\n  SidebarMenu,\r\n  SidebarMenuAction,\r\n  SidebarMenuButton,\r\n  SidebarMenuItem,\r\n  useSidebar,\r\n} from \"@/registry/new-york/ui/sidebar\"\r\n\r\ndefineProps<{\r\n  favorites: {\r\n    name: string\r\n    url: string\r\n    emoji: string\r\n  }[]\r\n}>()\r\n\r\nconst { isMobile } = useSidebar()\r\n</script>\r\n\r\n<template>\r\n  <SidebarGroup class=\"group-data-[collapsible=icon]:hidden\">\r\n    <SidebarGroupLabel>Favorites</SidebarGroupLabel>\r\n    <SidebarMenu>\r\n      <SidebarMenuItem v-for=\"item in favorites\" :key=\"item.name\">\r\n        <SidebarMenuButton as-child>\r\n          <a :href=\"item.url\" :title=\"item.name\">\r\n            <span>{{ item.emoji }}</span>\r\n            <span>{{ item.name }}</span>\r\n          </a>\r\n        </SidebarMenuButton>\r\n        <DropdownMenu>\r\n          <DropdownMenuTrigger as-child>\r\n            <SidebarMenuAction show-on-hover>\r\n              <MoreHorizontal />\r\n              <span class=\"sr-only\">More</span>\r\n            </SidebarMenuAction>\r\n          </DropdownMenuTrigger>\r\n          <DropdownMenuContent\r\n            class=\"w-56 rounded-lg\"\r\n            :side=\"isMobile ? 'bottom' : 'right'\"\r\n            :align=\"isMobile ? 'end' : 'start'\"\r\n          >\r\n            <DropdownMenuItem>\r\n              <StarOff class=\"text-muted-foreground\" />\r\n              <span>Remove from Favorites</span>\r\n            </DropdownMenuItem>\r\n            <DropdownMenuSeparator />\r\n            <DropdownMenuItem>\r\n              <Link class=\"text-muted-foreground\" />\r\n              <span>Copy Link</span>\r\n            </DropdownMenuItem>\r\n            <DropdownMenuItem>\r\n              <ArrowUpRight class=\"text-muted-foreground\" />\r\n              <span>Open in New Tab</span>\r\n            </DropdownMenuItem>\r\n            <DropdownMenuSeparator />\r\n            <DropdownMenuItem>\r\n              <Trash2 class=\"text-muted-foreground\" />\r\n              <span>Delete</span>\r\n            </DropdownMenuItem>\r\n          </DropdownMenuContent>\r\n        </DropdownMenu>\r\n      </SidebarMenuItem>\r\n\r\n      <SidebarMenuItem>\r\n        <SidebarMenuButton class=\"text-sidebar-foreground/70\">\r\n          <MoreHorizontal />\r\n          <span>More</span>\r\n        </SidebarMenuButton>\r\n      </SidebarMenuItem>\r\n    </SidebarMenu>\r\n  </SidebarGroup>\r\n</template>\r\n",
      "type": "registry:component",
      "target": ""
    },
    {
      "path": "blocks/Sidebar15/components/NavMain.vue",
      "content": "<script setup lang=\"ts\">\r\nimport type { LucideIcon } from \"lucide-vue-next\"\r\n\r\nimport {\r\n  SidebarMenu,\r\n  SidebarMenuButton,\r\n  SidebarMenuItem,\r\n} from \"@/registry/new-york/ui/sidebar\"\r\n\r\ndefineProps<{\r\n  items: {\r\n    title: string\r\n    url: string\r\n    icon: LucideIcon\r\n    isActive?: boolean\r\n  }[]\r\n}>()\r\n</script>\r\n\r\n<template>\r\n  <SidebarMenu>\r\n    <SidebarMenuItem v-for=\"item in items\" :key=\"item.title\">\r\n      <SidebarMenuButton as-child :is-active=\"item.isActive\">\r\n        <a :href=\"item.url\">\r\n          <component :is=\"item.icon\" />\r\n          <span>{{ item.title }}</span>\r\n        </a>\r\n      </SidebarMenuButton>\r\n    </SidebarMenuItem>\r\n  </SidebarMenu>\r\n</template>\r\n",
      "type": "registry:component",
      "target": ""
    },
    {
      "path": "blocks/Sidebar15/components/NavSecondary.vue",
      "content": "<script setup lang=\"ts\">\r\nimport type { LucideIcon } from \"lucide-vue-next\"\r\n\r\nimport type { Component } from \"vue\"\r\nimport {\r\n  SidebarGroup,\r\n  SidebarGroupContent,\r\n  SidebarMenu,\r\n  SidebarMenuBadge,\r\n  SidebarMenuButton,\r\n  SidebarMenuItem,\r\n} from \"@/registry/new-york/ui/sidebar\"\r\n\r\ndefineProps<{\r\n  items: {\r\n    title: string\r\n    url: string\r\n    icon: LucideIcon\r\n    badge?: Component\r\n  }[]\r\n}>()\r\n</script>\r\n\r\n<template>\r\n  <SidebarGroup>\r\n    <SidebarGroupContent>\r\n      <SidebarMenu>\r\n        <SidebarMenuItem v-for=\"item in items\" :key=\"item.title\">\r\n          <SidebarMenuButton as-child>\r\n            <a :href=\"item.url\">\r\n              <component :is=\"item.icon\" />\r\n              <span>{{ item.title }}</span>\r\n            </a>\r\n          </SidebarMenuButton>\r\n          <SidebarMenuBadge v-if=\"item.badge\">\r\n            <component :is=\"item.badge\" />\r\n          </SidebarMenuBadge>\r\n        </SidebarMenuItem>\r\n      </SidebarMenu>\r\n    </SidebarGroupContent>\r\n  </SidebarGroup>\r\n</template>\r\n",
      "type": "registry:component",
      "target": ""
    },
    {
      "path": "blocks/Sidebar15/components/NavUser.vue",
      "content": "<script setup lang=\"ts\">\r\nimport {\r\n  BadgeCheck,\r\n  Bell,\r\n  ChevronsUpDown,\r\n  CreditCard,\r\n  LogOut,\r\n  Sparkles,\r\n} from \"lucide-vue-next\"\r\n\r\nimport {\r\n  Avatar,\r\n  AvatarFallback,\r\n  AvatarImage,\r\n} from \"@/registry/new-york/ui/avatar\"\r\nimport {\r\n  DropdownMenu,\r\n  DropdownMenuContent,\r\n  DropdownMenuGroup,\r\n  DropdownMenuItem,\r\n  DropdownMenuLabel,\r\n  DropdownMenuSeparator,\r\n  DropdownMenuTrigger,\r\n} from \"@/registry/new-york/ui/dropdown-menu\"\r\nimport {\r\n  SidebarMenu,\r\n  SidebarMenuButton,\r\n  SidebarMenuItem,\r\n  useSidebar,\r\n} from \"@/registry/new-york/ui/sidebar\"\r\n\r\nconst props = defineProps<{\r\n  user: {\r\n    name: string\r\n    email: string\r\n    avatar: string\r\n  }\r\n}>()\r\n\r\nconst { isMobile } = useSidebar()\r\n</script>\r\n\r\n<template>\r\n  <SidebarMenu>\r\n    <SidebarMenuItem>\r\n      <DropdownMenu>\r\n        <DropdownMenuTrigger as-child>\r\n          <SidebarMenuButton\r\n            size=\"lg\"\r\n            class=\"data-[state=open]:bg-sidebar-accent data-[state=open]:text-sidebar-accent-foreground\"\r\n          >\r\n            <Avatar class=\"h-8 w-8 rounded-lg\">\r\n              <AvatarImage :src=\"user.avatar\" :alt=\"user.name\" />\r\n              <AvatarFallback class=\"rounded-lg\">\r\n                CN\r\n              </AvatarFallback>\r\n            </Avatar>\r\n            <div class=\"grid flex-1 text-left text-sm leading-tight\">\r\n              <span class=\"truncate font-medium\">{{ user.name }}</span>\r\n              <span class=\"truncate text-xs\">{{ user.email }}</span>\r\n            </div>\r\n            <ChevronsUpDown class=\"ml-auto size-4\" />\r\n          </SidebarMenuButton>\r\n        </DropdownMenuTrigger>\r\n        <DropdownMenuContent\r\n          class=\"w-[--reka-dropdown-menu-trigger-width] min-w-56 rounded-lg\"\r\n          :side=\"isMobile ? 'bottom' : 'right'\"\r\n          align=\"start\"\r\n          :side-offset=\"4\"\r\n        >\r\n          <DropdownMenuLabel class=\"p-0 font-normal\">\r\n            <div class=\"flex items-center gap-2 px-1 py-1.5 text-left text-sm\">\r\n              <Avatar class=\"h-8 w-8 rounded-lg\">\r\n                <AvatarImage :src=\"user.avatar\" :alt=\"user.name\" />\r\n                <AvatarFallback class=\"rounded-lg\">\r\n                  CN\r\n                </AvatarFallback>\r\n              </Avatar>\r\n              <div class=\"grid flex-1 text-left text-sm leading-tight\">\r\n                <span class=\"truncate font-medium\">{{ user.name }}</span>\r\n                <span class=\"truncate text-xs\">{{ user.email }}</span>\r\n              </div>\r\n            </div>\r\n          </DropdownMenuLabel>\r\n          <DropdownMenuSeparator />\r\n          <DropdownMenuGroup>\r\n            <DropdownMenuItem>\r\n              <Sparkles />\r\n              Upgrade to Pro\r\n            </DropdownMenuItem>\r\n          </DropdownMenuGroup>\r\n          <DropdownMenuSeparator />\r\n          <DropdownMenuGroup>\r\n            <DropdownMenuItem>\r\n              <BadgeCheck />\r\n              Account\r\n            </DropdownMenuItem>\r\n            <DropdownMenuItem>\r\n              <CreditCard />\r\n              Billing\r\n            </DropdownMenuItem>\r\n            <DropdownMenuItem>\r\n              <Bell />\r\n              Notifications\r\n            </DropdownMenuItem>\r\n          </DropdownMenuGroup>\r\n          <DropdownMenuSeparator />\r\n          <DropdownMenuItem>\r\n            <LogOut />\r\n            Log out\r\n          </DropdownMenuItem>\r\n        </DropdownMenuContent>\r\n      </DropdownMenu>\r\n    </SidebarMenuItem>\r\n  </SidebarMenu>\r\n</template>\r\n",
      "type": "registry:component",
      "target": ""
    },
    {
      "path": "blocks/Sidebar15/components/NavWorkspaces.vue",
      "content": "<script setup lang=\"ts\">\r\nimport { ChevronRight, MoreHorizontal, Plus } from \"lucide-vue-next\"\r\n\r\nimport {\r\n  Collapsible,\r\n  CollapsibleContent,\r\n  CollapsibleTrigger,\r\n} from \"@/registry/new-york/ui/collapsible\"\r\nimport {\r\n  SidebarGroup,\r\n  SidebarGroupContent,\r\n  SidebarGroupLabel,\r\n  SidebarMenu,\r\n  SidebarMenuAction,\r\n  SidebarMenuButton,\r\n  SidebarMenuItem,\r\n  SidebarMenuSub,\r\n  SidebarMenuSubButton,\r\n  SidebarMenuSubItem,\r\n} from \"@/registry/new-york/ui/sidebar\"\r\n\r\ndefineProps<{\r\n  workspaces: {\r\n    name: string\r\n    emoji: string\r\n    pages: {\r\n      name: string\r\n      emoji: string\r\n    }[]\r\n  }[]\r\n}>()\r\n</script>\r\n\r\n<template>\r\n  <SidebarGroup>\r\n    <SidebarGroupLabel>Workspaces</SidebarGroupLabel>\r\n    <SidebarGroupContent>\r\n      <SidebarMenu>\r\n        <Collapsible v-for=\"workspace in workspaces\" :key=\"workspace.name\">\r\n          <SidebarMenuItem>\r\n            <SidebarMenuButton as-child>\r\n              <a href=\"#\">\r\n                <span>{{ workspace.emoji }}</span>\r\n                <span>{{ workspace.name }}</span>\r\n              </a>\r\n            </SidebarMenuButton>\r\n            <CollapsibleTrigger as-child>\r\n              <SidebarMenuAction\r\n                class=\"left-2 bg-sidebar-accent text-sidebar-accent-foreground data-[state=open]:rotate-90\"\r\n                show-on-hover\r\n              >\r\n                <ChevronRight />\r\n              </SidebarMenuAction>\r\n            </CollapsibleTrigger>\r\n            <SidebarMenuAction show-on-hover>\r\n              <Plus />\r\n            </SidebarMenuAction>\r\n            <CollapsibleContent>\r\n              <SidebarMenuSub>\r\n                <SidebarMenuSubItem v-for=\"page in workspace.pages\" :key=\"page.name\">\r\n                  <SidebarMenuSubButton as-child>\r\n                    <a href=\"#\">\r\n                      <span>{{ page.emoji }}</span>\r\n                      <span>{{ page.name }}</span>\r\n                    </a>\r\n                  </SidebarMenuSubButton>\r\n                </SidebarMenuSubItem>\r\n              </SidebarMenuSub>\r\n            </CollapsibleContent>\r\n          </SidebarMenuItem>\r\n        </Collapsible>\r\n\r\n        <SidebarMenuItem>\r\n          <SidebarMenuButton class=\"text-sidebar-foreground/70\">\r\n            <MoreHorizontal />\r\n            <span>More</span>\r\n          </SidebarMenuButton>\r\n        </SidebarMenuItem>\r\n      </SidebarMenu>\r\n    </SidebarGroupContent>\r\n  </SidebarGroup>\r\n</template>\r\n",
      "type": "registry:component",
      "target": ""
    },
