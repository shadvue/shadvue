    {
      "path": "blocks/Products01/components/ProductsTable.vue",
      "content": "<script setup lang=\"ts\">\r\nimport {\r\n  ArrowUpDownIcon,\r\n  EllipsisVerticalIcon,\r\n  ListFilterIcon,\r\n  PlusIcon,\r\n} from \"lucide-vue-next\"\r\n\r\nimport { Badge } from \"@/registry/new-york/ui/badge\"\r\nimport { Button } from \"@/registry/new-york/ui/button\"\r\nimport { Checkbox } from \"@/registry/new-york/ui/checkbox\"\r\nimport {\r\n  DropdownMenu,\r\n  DropdownMenuContent,\r\n  DropdownMenuItem,\r\n  DropdownMenuTrigger,\r\n} from \"@/registry/new-york/ui/dropdown-menu\"\r\nimport {\r\n  Pagination,\r\n  PaginationContent,\r\n  PaginationEllipsis,\r\n  PaginationItem,\r\n  PaginationNext,\r\n  PaginationPrevious,\r\n} from \"@/registry/new-york/ui/pagination\"\r\nimport {\r\n  Select,\r\n  SelectContent,\r\n  SelectItem,\r\n  SelectTrigger,\r\n  SelectValue,\r\n} from \"@/registry/new-york/ui/select\"\r\nimport {\r\n  Table,\r\n  TableBody,\r\n  TableCell,\r\n  TableHead,\r\n  TableHeader,\r\n  TableRow,\r\n} from \"@/registry/new-york/ui/table\"\r\nimport { Tabs, TabsList, TabsTrigger } from \"@/registry/new-york/ui/tabs\"\r\n\r\nconst props = defineProps<{\r\n  products: {\r\n    id: string\r\n    name: string\r\n    price: number\r\n    stock: number\r\n    dateAdded: string\r\n    status: string\r\n  }[]\r\n}>()\r\n</script>\r\n\r\n<template>\r\n  <div class=\"flex w-full flex-col gap-4\">\r\n    <div class=\"flex items-center justify-between gap-4\">\r\n      <Tabs default-value=\"all\">\r\n        <TabsList>\r\n          <TabsTrigger value=\"all\">\r\n            All Products\r\n          </TabsTrigger>\r\n          <TabsTrigger value=\"in-stock\">\r\n            In Stock\r\n          </TabsTrigger>\r\n          <TabsTrigger value=\"low-stock\">\r\n            Low Stock\r\n          </TabsTrigger>\r\n          <TabsTrigger value=\"archived\">\r\n            Archived\r\n          </TabsTrigger>\r\n          <TabsTrigger value=\"add-product\" as-child>\r\n            <button>\r\n              <PlusIcon />\r\n            </button>\r\n          </TabsTrigger>\r\n        </TabsList>\r\n      </Tabs>\r\n      <div class=\"flex items-center gap-2 **:data-[slot=button]:size-8 **:data-[slot=select-trigger]:h-8\">\r\n        <Select default-value=\"all\">\r\n          <SelectTrigger>\r\n            <span class=\"text-muted-foreground text-sm\">Category:</span>\r\n            <SelectValue placeholder=\"Select a product\" />\r\n          </SelectTrigger>\r\n          <SelectContent>\r\n            <SelectItem value=\"all\">\r\n              All\r\n            </SelectItem>\r\n            <SelectItem value=\"in-stock\">\r\n              In Stock\r\n            </SelectItem>\r\n            <SelectItem value=\"low-stock\">\r\n              Low Stock\r\n            </SelectItem>\r\n            <SelectItem value=\"archived\">\r\n              Archived\r\n            </SelectItem>\r\n          </SelectContent>\r\n        </Select>\r\n        <Select default-value=\"all\">\r\n          <SelectTrigger>\r\n            <span class=\"text-muted-foreground text-sm\">Price:</span>\r\n            <SelectValue placeholder=\"Select a product\" />\r\n          </SelectTrigger>\r\n          <SelectContent>\r\n            <SelectItem value=\"all\">\r\n              $100-$200\r\n            </SelectItem>\r\n            <SelectItem value=\"in-stock\">\r\n              $200-$300\r\n            </SelectItem>\r\n            <SelectItem value=\"low-stock\">\r\n              $300-$400\r\n            </SelectItem>\r\n            <SelectItem value=\"archived\">\r\n              $400-$500\r\n            </SelectItem>\r\n          </SelectContent>\r\n        </Select>\r\n        <Select default-value=\"all\">\r\n          <SelectTrigger>\r\n            <span class=\"text-muted-foreground text-sm\">Status:</span>\r\n            <SelectValue placeholder=\"Select a product\" />\r\n          </SelectTrigger>\r\n          <SelectContent>\r\n            <SelectItem value=\"all\">\r\n              In Stock\r\n            </SelectItem>\r\n            <SelectItem value=\"in-stock\">\r\n              Low Stock\r\n            </SelectItem>\r\n            <SelectItem value=\"low-stock\">\r\n              Archived\r\n            </SelectItem>\r\n            <SelectItem value=\"archived\">\r\n              Archived\r\n            </SelectItem>\r\n          </SelectContent>\r\n        </Select>\r\n        <Button variant=\"outline\" size=\"icon\">\r\n          <ListFilterIcon />\r\n        </Button>\r\n        <Button variant=\"outline\" size=\"icon\">\r\n          <ArrowUpDownIcon />\r\n        </Button>\r\n      </div>\r\n    </div>\r\n    <div class=\"rounded-lg\">\r\n      <Table>\r\n        <TableHeader class=\"bg-muted/50\">\r\n          <TableRow class=\"!border-0\">\r\n            <TableHead class=\"w-12 rounded-l-lg px-4\">\r\n              <Checkbox />\r\n            </TableHead>\r\n            <TableHead>Product</TableHead>\r\n            <TableHead class=\"text-right\">\r\n              Price\r\n            </TableHead>\r\n            <TableHead class=\"text-right\">\r\n              Stock\r\n            </TableHead>\r\n            <TableHead>Status</TableHead>\r\n            <TableHead>Date Added</TableHead>\r\n            <TableHead class=\"rounded-r-lg\" />\r\n          </TableRow>\r\n        </TableHeader>\r\n        <TableBody class=\"**:data-[slot=table-cell]:py-2.5\">\r\n          <TableRow v-for=\"product in products\" :key=\"product.id\">\r\n            <TableCell class=\"px-4\">\r\n              <Checkbox />\r\n            </TableCell>\r\n            <TableCell class=\"font-medium\">\r\n              {{ product.name }}\r\n            </TableCell>\r\n            <TableCell class=\"text-right\">\r\n              ${{ product.price.toFixed(2) }}\r\n            </TableCell>\r\n            <TableCell class=\"text-right\">\r\n              {{ product.stock }}\r\n            </TableCell>\r\n            <TableCell>\r\n              <Badge\r\n                variant=\"secondary\"\r\n                :class=\"\r\n                  product.status === 'Low Stock'\r\n                    ? 'border-orange-500 bg-transparent text-orange-500 dark:border-orange-500 dark:bg-transparent dark:text-orange-500'\r\n                    : 'bg-green-100 text-green-800 dark:bg-green-900 dark:text-green-100'\r\n                \"\r\n              >\r\n                {{ product.status }}\r\n              </Badge>\r\n            </TableCell>\r\n            <TableCell>\r\n              {{ new Date(product.dateAdded).toLocaleDateString(\"en-US\", {\r\n                month: \"long\",\r\n                day: \"numeric\",\r\n                year: \"numeric\",\r\n              }) }}\r\n            </TableCell>\r\n            <TableCell>\r\n              <DropdownMenu>\r\n                <DropdownMenuTrigger as-child>\r\n                  <Button variant=\"ghost\" size=\"icon\" class=\"size-6\">\r\n                    <EllipsisVerticalIcon />\r\n                  </Button>\r\n                </DropdownMenuTrigger>\r\n                <DropdownMenuContent align=\"end\">\r\n                  <DropdownMenuItem>Edit</DropdownMenuItem>\r\n                  <DropdownMenuItem variant=\"destructive\">\r\n                    Delete\r\n                  </DropdownMenuItem>\r\n                </DropdownMenuContent>\r\n              </DropdownMenu>\r\n            </TableCell>\r\n          </TableRow>\r\n        </TableBody>\r\n      </Table>\r\n    </div>\r\n    <div class=\"flex justify-end\">\r\n      <Pagination :items-per-page=\"10\">\r\n        <PaginationContent>\r\n          <PaginationPrevious href=\"#\" />\r\n          <PaginationItem :value=\"1\">\r\n            1\r\n          </PaginationItem>\r\n          <PaginationItem :value=\"2\">\r\n            2\r\n          </PaginationItem>\r\n          <PaginationItem :value=\"3\">\r\n            3\r\n          </PaginationItem>\r\n\r\n          <PaginationEllipsis />\r\n\r\n          <PaginationNext href=\"#\" />\r\n        </PaginationContent>\r\n      </Pagination>\r\n    </div>\r\n  </div>\r\n</template>\r\n",
      "type": "registry:component",
      "target": ""
    }
  ]
}
