center justify-center rounded-lg bg-sidebar-primary text-sidebar-primary-foreground\">\r\n                <GalleryVerticalEnd class=\"size-4\" />\r\n              </div>\r\n              <div class=\"flex flex-col gap-0.5 leading-none\">\r\n                <span class=\"font-medium\">Documentation</span>\r\n                <span class=\"\">v1.0.0</span>\r\n              </div>\r\n            </a>\r\n          </SidebarMenuButton>\r\n        </SidebarMenuItem>\r\n      </SidebarMenu>\r\n      <SearchForm />\r\n    </SidebarHeader>\r\n    <SidebarContent>\r\n      <SidebarGroup>\r\n        <SidebarMenu>\r\n          <Collapsible\r\n            v-for=\"(item, index) in data.navMain\"\r\n            :key=\"item.title\"\r\n            :default-open=\"index === 1\"\r\n            class=\"group/collapsible\"\r\n          >\r\n            <SidebarMenuItem>\r\n              <CollapsibleTrigger as-child>\r\n                <SidebarMenuButton>\r\n                  {{ item.title }}\r\n                  <Plus class=\"ml-auto group-data-[state=open]/collapsible:hidden\" />\r\n                  <Minus class=\"ml-auto group-data-[state=closed]/collapsible:hidden\" />\r\n                </SidebarMenuButton>\r\n              </CollapsibleTrigger>\r\n              <CollapsibleContent v-if=\"item.items.length\">\r\n                <SidebarMenuSub>\r\n                  <SidebarMenuSubItem v-for=\"childItem in item.items\" :key=\"childItem.title\">\r\n                    <SidebarMenuSubButton\r\n                      as-child\r\n                      :is-active=\"childItem.isActive\"\r\n                    >\r\n                      <a :href=\"childItem.url\">{{ childItem.title }}</a>\r\n                    </SidebarMenuSubButton>\r\n                  </SidebarMenuSubItem>\r\n                </SidebarMenuSub>\r\n              </CollapsibleContent>\r\n            </SidebarMenuItem>\r\n          </Collapsible>\r\n        </SidebarMenu>\r\n      </SidebarGroup>\r\n    </SidebarContent>\r\n    <SidebarRail />\r\n  </Sidebar>\r\n</template>\r\n",
